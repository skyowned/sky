<apex:page >
    <html> 
       	<video width="640" height="360" controls="true" autoplay="true">
            <!-- MP4 must be first for iPad! -->
            <source src="https://sb-allergan-promomats-sbx.veevavault.com/ui/#doc_info/818" type="video/mp4"/><!-- Safari / iOS, IE9 -->
            <source src="https://sb-allergan-promomats-sbx.veevavault.com/ui/#doc_info/818" type="video/webm"/><!-- Chrome10+, Ffx4+, Opera10.6+ -->
            <source src="https://sb-allergan-promomats-sbx.veevavault.com/ui/#doc_info/818" type="video/ogg"/><!-- Firefox3.6+ / Opera 10.5+ -->
            <!-- fallback to Flash: -->
            <object width="640" height="360" type="application/x-shockwave-flash" data="player.swf">
                <!-- Firefox uses the `data` attribute above, IE/Safari uses the param below -->
                <param name="movie" value="player.swf"/>
                <param name="flashvars" value="autostart=true&amp;controlbar=over&amp;image=poster.jpg&amp;file=https://sb-allergan-promomats-sbx.veevavault.com/ui/#doc_info/818"/>
                <!-- fallback image -->
                <img src="poster.jpg" width="640" height="360" alt="Big Buck Bunny" title="No video playback capabilities, please download the video below"/>
            </object>
        </video>
    </html>
</apex:page>